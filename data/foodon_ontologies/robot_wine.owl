                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0001000"/>
                        <owl:allValuesFrom rdf:resource="http://purl.obolibrary.org/obo/FOODON_00002604"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Sangiovese wine is a red wine varietal produced only from Sangiovese grapes. It is prized for its high acid, firm tannins and balanced nature. Savory flavors of dark cherries and black stonefruit are characteristic, and may be backed by secondary notes of tomato leaf and dried herbs.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ginny Pichler</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.wine-searcher.com/grape-432-sangiovese</obo:IAO_0000119>
        <schema:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/2/2e/Sangiovese_wine.jpg/180px-Sangiovese_wine.jpg"/>
        <rdfs:label xml:lang="en">Sangiovese wine</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FOODON_00002604 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FOODON_00002604">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FOODON_03412121"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Sangiovese is a red Italian wine grape variety that derives its name from the Latin sanguis Jovis, &quot;the blood of Jupiter&quot;.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ginny Pichler</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Sangiovese</obo:IAO_0000119>
        <schema:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/3/37/Sangiovese_close_up_crop.jpg/277px-Sangiovese_close_up_crop.jpg"/>
        <rdfs:label xml:lang="en">Sangiovese grape plant</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FOODON_00002605 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FOODON_00002605">
