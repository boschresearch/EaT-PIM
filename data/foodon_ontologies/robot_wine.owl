    <!-- http://purl.obolibrary.org/obo/FOODON_00002610 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FOODON_00002610">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FOODON_03412121"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Glera is a white variety of grape of Slovenian origin, which was brought to the village of Prosecco from Slovenia&apos;s Karst region. Glera was, until 2009, mostly referred to as Prosecco. It is mainly cultivated for use in sparkling Italian wine styles.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ginny Pichler</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Glera_(grape)</obo:IAO_0000119>
        <schema:image rdf:resource="https://cdn1.wine-searcher.net/images/grape/glera-(prosecco)-392-1-1.jpg"/>
        <oboInOwl:hasExactSynonym xml:lang="en">Prosecco grape</oboInOwl:hasExactSynonym>
        <rdfs:label xml:lang="en">Glera grape plant</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FOODON_00002611 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FOODON_00002611">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FOODON_03310273"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0001000"/>
                        <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/FOODON_00002610"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0001000"/>
                        <owl:allValuesFrom rdf:resource="http://purl.obolibrary.org/obo/FOODON_00002610"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Prosecco is an Italian white wine varietal produced primarily from Glera grapes, but other grape varieties may be included. Prosecco controlled designation of origin can be spumante (&quot;sparkling wine&quot;), frizzante (&quot;semi-sparkling wine&quot;), or tranquillo (&quot;still wine&quot;).</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ginny Pichler</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Prosecco</obo:IAO_0000119>
        <schema:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/c/ce/Prosecco_di_Conegliano_bottle_and_glass.jpg/151px-Prosecco_di_Conegliano_bottle_and_glass.jpg"/>
        <oboInOwl:hasExactSynonym xml:lang="it">frizzante</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym xml:lang="it">spumante</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym xml:lang="it">tranquillo</oboInOwl:hasExactSynonym>
        <rdfs:label xml:lang="en">Prosecco wine</rdfs:label>
    </owl:Class>
    


