        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FOODON_03412121"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Semillon is a golden-skinned grape used to make dry and sweet white wines, mostly in France and Australia.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ginny Pichler</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/S%C3%A9millon</obo:IAO_0000119>
        <schema:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/2/27/Semillon_starting_to_get_noble_rot_at_Ch%C3%A2teau_Doisy-V%C3%A9drines%2C_Barsac%2C_Sauternes.jpg/160px-Semillon_starting_to_get_noble_rot_at_Ch%C3%A2teau_Doisy-V%C3%A9drines%2C_Barsac%2C_Sauternes.jpg"/>
        <oboInOwl:hasExactSynonym xml:lang="fr">Sémillon grape</oboInOwl:hasExactSynonym>
        <rdfs:label xml:lang="en">Semillon grape plant</rdfs:label>
    </owl:Class>
    


