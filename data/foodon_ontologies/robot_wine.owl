        </rdfs:subClassOf>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Semillon wine is a white wine varietal produced only from Semillon grapes.  The primary fruit flavors in Semillon wine are lemon, apple, pear and green papaya. Semillon is France’s 3rd most planted white wine variety behind Sauvignon Blanc and Chardonnay.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ginny Pichler</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://winefolly.com/review/have-you-heard-of-semillon-the-3rd-most-important-french-white-wine/</obo:IAO_0000119>
        <schema:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/1/1d/Late_harvest_Semillon.jpg/179px-Late_harvest_Semillon.jpg"/>
        <oboInOwl:hasExactSynonym xml:lang="fr">Sémillon wine</oboInOwl:hasExactSynonym>
        <rdfs:label xml:lang="en">Semillon wine</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FOODON_00002608 -->

