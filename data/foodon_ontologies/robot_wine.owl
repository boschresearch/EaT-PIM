        <obo:IAO_0000115 xml:lang="en">Merlot is a dark blue-colored wine grape variety, that is used as both a blending grape and for varietal wines.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lauren Tindale</obo:IAO_0000117>
