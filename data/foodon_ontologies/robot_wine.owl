        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Riesling grape is an aromatic white grape variety displaying flowery, almost perfumed, aromas as well as high acidity.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ginny Pichler</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Riesling</obo:IAO_0000119>
        <schema:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/7/76/Riesling_grapes_leaves.jpg/320px-Riesling_grapes_leaves.jpg"/>
        <rdfs:label xml:lang="en">Riesling grape plant</rdfs:label>
    </owl:Class>
    


