    <!-- http://purl.obolibrary.org/obo/FOODON_00002587 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FOODON_00002587">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/FOODON_03412121"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Chenin blanc grapes are a White wine grape variety from the Loire Valley of France. Its high acidity means it can be used to make everything from sparkling wines to well-balanced dessert wines, although it can produce very bland, neutral wines if the vine&apos;s natural vigor is not controlled.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lauren Tindale</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Chenin_blanc</obo:IAO_0000119>
        <oboInOwl:hasExactSynonym xml:lang="fr">Pineau de la Loire</oboInOwl:hasExactSynonym>
        <rdfs:label xml:lang="en">Chenin blanc grape plant</rdfs:label>
    </owl:Class>
    


