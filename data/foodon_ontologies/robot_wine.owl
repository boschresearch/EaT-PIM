        </rdfs:subClassOf>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000122"/>
        <obo:IAO_0000115 xml:lang="en">Riesling wine is a white wine varietal produced only from Riesling grapes, and offered as dry, semi-sweet, sweet, and sparkling white wines. Riesling wines are usually varietally pure and are seldom oaked.</obo:IAO_0000115>
        <obo:IAO_0000117 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ginny Pichler</obo:IAO_0000117>
        <obo:IAO_0000119 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Riesling</obo:IAO_0000119>
        <schema:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/9/96/Alsatian_Riesling_.jpg/179px-Alsatian_Riesling_.jpg"/>
        <rdfs:label xml:lang="en">Riesling wine</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/FOODON_00002602 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/FOODON_00002602">
