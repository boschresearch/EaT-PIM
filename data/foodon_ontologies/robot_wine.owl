        <schema:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/e/ee/Merlot_Grape.jpg/320px-Merlot_Grape.jpg"/>
        <rdfs:label xml:lang="en">Merlot grape plant</rdfs:label>
    </owl:Class>
    


